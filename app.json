{
  "expo": {
    "name": "Knight Hacks",
    "entryPoint": "./src/App.tsx",
    "slug": "hackathon-app-2021",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.knighthacks.knighthacks",
      "buildNumber": "1.0.1",
      "supportsTablet": false,
      "infoPlist": {
        "NSRemindersUsageDescription": "The calendar is accessed to add hackathon workshops."
      }
    },
    "android": {
      "package": "org.knighthacks.knighthacks",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["READ_CALENDAR", "WRITE_CALENDAR"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["sentry-expo"]
  }
}
